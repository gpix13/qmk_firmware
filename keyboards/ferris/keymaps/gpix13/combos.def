// Left top row
COMB(lcurly,               KC_LCBR,        KC_R, KC_T)

// Right top row
COMB(rcurly,               KC_RCBR,        KC_Y, KC_U)
COMB(eql,                  KC_EQL,         KC_U, KC_I)

// Left home row
COMB(lparen,               KC_LPRN,        LGUI_T(KC_F), KC_G)
COMB(dquo,                 KC_DQUO,        LCTL_T(KC_S), LGUI_T(KC_F))

// Right home row
COMB(rparen,               KC_RPRN,        KC_H, LGUI_T(KC_J))
COMB(squo,                 KC_QUOT,        LGUI_T(KC_J), LCTL_T(KC_L))
COMB(entr,                 KC_ENT,         LGUI_T(KC_J), LALT_T(KC_K))

// Left bottom row
COMB(lbracket,             KC_LBRC,        KC_V, KC_B)

// Right bottom row
COMB(rbracket,             KC_RBRC,        KC_N, KC_M)
COMB(tab,                  KC_TAB,         KC_M, KC_COMM)

// left handed combos with COMMAND
COMB(copy,                 G(KC_C),        MO(1), KC_C)
COMB(cut,                  G(KC_Z),        MO(1), KC_X)
COMB(paste,                G(KC_V),        MO(1), KC_V)
COMB(undo,                 G(KC_Z),        MO(1), KC_Z)
COMB(selectall,            G(KC_A),        MO(1), KC_A)
COMB(divvy,                G(KC_E),        MO(1), KC_E)
COMB(quit,                 G(KC_Q),        MO(1), KC_Q)

